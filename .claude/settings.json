{
  "permissions": {
    "allow": [
      "Bash(mvn test)",
      "Bash(mvn compile)",
      "Bash(mvn clean install)",
      "Bash(mvn package*)",
      "Bash(git status)",
      "Bash(git diff*)",
      "Bash(git log*)",
      "Bash(git add*)",
      "Bash(git commit*)"
    ],
    "deny": [
      "Bash(git push --force*)",
      "Bash(git reset --hard*)",
      "Bash(rm -rf*)",
      "Bash(git commit*--no-verify*)"
    ]
  }
}
